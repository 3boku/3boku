PNT VR, V(20)
INT I

B(4).1=0 : B(4).2=1 : WAIT 500
B(4).2=0 : B(4).0=0

SPD = 8000
TAG A
MPTP P1
STBY B(10).0==1
CALL PICK
MPTP P2
B(10).0=0
STBY B(10).0 == 1
	CALL WRITE01
	B(10).0=0
STBY B(10).0==1
	MINC <20.0>
	CALL WRITE02
	B(10).0=0
STBY B(10).0==1
	MINC <20.0>
	CALL WRITE02
	B(10).0=0
MPTP P1
CALL PLACE
CALL A
MEND

SPGM WRITE01
	VR = GPNT(512)
	FOR I=1 TO 10 : V(I)=VR : ENDF
	V(1).1=VR.1+5 : V(2)=V(1)
	V(2).2=VR.2 + 16
	MPTP V(1)
	CALL SOL1
	MPTP V(2)
	CALL SOL2
	MPTP VR
SEND
SPGM WRITE02
	VR=GPNT(512)
	FOR I=1 TO 10 : V(I)=VR : ENDF
	V(1).2=VR.2+3
	V(2).1=VR.1+3
	V(3).1=VR.1+7
	V(4).1=VR.1+10 : V(4).2=VR.2+3
	V(5).2=VR.2+16
	V(6).1=VR.1+10 : V(6).2=VR.2+16
	MPTP V(1)
	CALL SOL1
	FOR I=2 TO 6 : MPTP V(I) : ENDF
	CALL SOL2
	MPTP VR
SEND
SPGM WRITE03
	VR=GPNT(512)
	FOR I=1 TO 10 : V(I)=VR : ENDF
	V(1).2=VR.2+3
	V(2).1=VR.1+3
	V(3).1=VR.1+7
	V(4).1=VR.1+10 : V(4).2=VR.2+3
	V(5).2=VR.2+8 : V(5).1=VR.1+5
	V(6).1=VR.1+10 : V(6).2=VR.2+13
	V(7).1=VR.1+7 : V(7).2=VR.2+16
	V(8).1=VR.1+3 : V(8).2=VR.2+16
	V(9).2=VR.2+13
	MPTP V(1)
	CALL SOL1
	FOR I=2 TO 9 : MPTP V(I) : ENDF
	CALL SOL2
	MPTP VR
SEND
SPGM SOL1
	B(4).1=1 : WAIT 1000
	B(4).1=0 : STBY(B(0).0==1)
SEND
SPGM SOL2
	B(4).2=1 : WAIT 1000
	B(4).2=0 : STBY(B(0).1==1)
SEND
SPGM PICK
	CALL SOL1
	B(4).0 = 1: WAIT 1000 : STBY(B(0).3 == 1)
	CALL SOL2
SEND
SPGM PLACE
	CALL SOL1
	B(4).0=0 : WAIT 1000 : STBY(B(0).2 == 1)
	CALL SOL2
SEND